{
  "version": "2.0.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "reactjs",
    "input",
    "select",
    "react-currency-input-field",
    "react-select",
    "dropdown"
  ],
  "name": "@meksiabdou/react-input",
  "description": "html input for reactjs",
  "repository": "https://github.com/meksiabdou/react-input",
  "author": "Meksi Abdennour <meksiabdou@gmail.com> (https://github.com/meksiabdou)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meksiabdou/react-input/issues"
  },
  "homepage": "https://github.com/meksiabdou/react-input#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "module": "dist/react-input.esm.js",
  "size-limit": [
    {
      "path": "dist/react-input.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-input.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "husky": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "react-currency-input-field": "^3.6.4",
    "react-select": "^5.4.0"
  }
}
